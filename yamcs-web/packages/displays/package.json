{
  "name": "@yamcs/displays",
  "version": "1.0.0",
  "description": "Display Renderer for yamcs-web",
  "private": true,
  "main": "dist/displays.umd.js",
  "module": "dist/displays.es5.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Space Applications Services",
  "license": "AGPL-3.0",
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "tsc --module commonjs && rollup -c rollup.config.ts",
    "watch": "rollup -c rollup.config.ts -w",
    "test": "jest",
    "opi-pegjs": "cd src/opi/formulas && pegjs --plugin ts-pegjs/src/tspegjs -o parser.ts grammar.pegjs",
    "uss-pegjs": "cd src/uss/expressions && pegjs --plugin ts-pegjs/src/tspegjs -o parser.ts grammar.pegjs"
  },
  "dependencies": {
    "@yamcs/client": "^1.0.0",
    "dygraphs": "^2.1.0",
    "fontfaceobserver": "^2.0.13"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.0.0",
    "@types/pegjs": "^0.10.0",
    "jest": "^24.9.0",
    "pegjs": "^0.10.0",
    "rollup": "^1.0.1",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.25.2",
    "ts-jest": "^24.1.0",
    "ts-pegjs": "^0.2.1",
    "tslint": "^5.8.0",
    "typescript": "~3.5.3"
  }
}
